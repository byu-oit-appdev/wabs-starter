{
    "name": "contact-info",
    "version": "1.0.0",
    "description": "A WABS app",
    "main": "server/index.js",
    "scripts": {
        "start": "node $WABS_INSPECT index.js",
        "dev": "nodemon $WABS_INSPECT --watch index.js",
        "test": "mocha $WABS_INSPECT test/*.js",
        "coverage": "nyc mocha $WABS_INSPECT test/*.js"
    },
    "private": true,
    "author": "Aaron Rasmussen <aaron_rasmussen@byu.edu>",
    "license": "Apache-2.0",
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-env": "^1.5.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "bower": "^1.8.0",
        "css-loader": "^0.28.4",
        "chai": "^3.5.0",
        "chokidar": "^1.7.0",
        "mocha": "^3.2.0",
        "nyc": "^10.0.0",
        "vue-loader": "^12.2.1",
        "vue-template-compiler": "^2.3.4",
        "webpack": "^2.7.0",
        "webpack-dev-middleware": "^1.11.0",
        "webpack-hot-middleware": "^2.18.2"
    },
    "dependencies": {
        "body-parser": "^1.17.2",
        "byu-wabs": "^4.0.1",
        "byu-wso2-request": "github:byu-oit/byu-wso2-request",
        "cookie-parser": "^1.4.3",
        "debug": "^2.6.8",
        "express": "^4.15.3",
        "lodash": "^4.17.4",
        "promise-polyfill": "^6.0.2",
        "redux": "^3.7.2",
        "revue": "^3.0.0",
        "vue": "^2.4.2",
        "vue-router": "^2.7.0",
        "whatwg-fetch": "^2.0.3"
    },
    "babel": {
        "plugins": [
            [
                "transform-object-rest-spread",
                {
                    "useBuiltIns": true
                }
            ]
        ],
        "presets": [
            [
                "env",
                {
                    "modules": false
                }
            ]
        ]
    },
    "wabs": true
}
